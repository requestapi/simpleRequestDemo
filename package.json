{
  "name": "typescript-react-redux-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "A frontend boilerplate with React, Redux and Typescript",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run build && DEBUG=app node dist/server.js",
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "npx eslint './**/*.ts'",
    "lint:fix": "npx eslint --fix './**/*.ts'",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\""
  },
  "engines": {
    "node": "12"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/circular-json": "^0.4.0",
    "@types/debug": "0.0.30",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.17.9",
    "@types/history": "^4.7.8",
    "@types/http-errors": "^1.8.0",
    "@types/log4js": "^2.3.5",
    "@types/morgan": "^1.9.2",
    "@types/request": "^2.48.5",
    "@types/uuid": "^3.4.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "axios-logger": "^0.1.2",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1"
  },
  "dependencies": {
    "@types/node": "^10.17.46",
    "axios": "^0.18.0",
    "axios-debug-log": "^0.4.0",
    "body-parser": "^1.18.3",
    "colors": "^1.3.2",
    "crypto": "^1.0.1",
    "debug": "^3.2.7",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "http-errors": "^1.8.0",
    "http-status-codes": "^1.4.0",
    "lawgs": "^1.0.4",
    "log4js": "^3.0.5",
    "moment": "^2.29.1",
    "morgan": "^1.9.0",
    "rimraf": "^2.6.2",
    "typescript": "^4.1.2",
    "uuid": "^3.3.2"
  }
}
